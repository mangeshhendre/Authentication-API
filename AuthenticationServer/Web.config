<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <authentication mode="Windows" />
    <urlMappings enabled="true">
      <add url="~/" mappedUrl="~/BasicAuthenticate" />
      <add url="~/Basic" mappedUrl="~/BasicAuthenticate" />
      <add url="~/Token" mappedUrl="~/JWTAuthenticate" />
      <add url="~/Windows" mappedUrl="~/WindowsAuthenticate" />
    </urlMappings>
    <authorization>
      <allow roles="sgpd\Domain Users" />
      <deny users="*" />
    </authorization>
  </system.web>
  <location path="BasicAuthenticate">
    <system.web>
      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
  <location path="JWTAuthenticate">
    <system.web>
      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
  <location path="Certificate">
    <system.web>
      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
  
  <runtime>
